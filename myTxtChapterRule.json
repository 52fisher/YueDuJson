[
  {
    "enable": true,
    "name": "数字 分隔符 标题名称",
    "rule": "^\\s{0,4}\\d{1,5}[\\,\\.， 、\\-].{1,30}$",
    "serialNumber": 3
  },
  {
    "enable": true,
    "name": "特殊符号 序号 标题",
    "rule": "^\\s{0,4}[\\[〈「『〖〔《（【\\(]?[第卷][\\d零一二两三四五六七八九十百千万壹贰叁肆伍陆柒捌玖拾佰仟]{1,10}[\\.:： \f\t].{0,30}$",
    "serialNumber": 4
  },
  {
    "enable": true,
    "name": "特殊符号 标题",
    "rule": "^\\s{0,4}[\\[〈「『〖〔《（【\\(☆★].{1,30}[\\)】）》〕〗』」〉\\]]?\\s{0,4}$",
    "serialNumber": 5
  },
  {
    "enable": true,
    "name": "特殊符号 标题(不匹配空白字符)",
    "rule": "(?<=\\s)[\\[〈「『〖〔《（【\\(☆★].{1,30}[\\)】）》〕〗』」〉\\]]?\\s{0,4}$",
    "serialNumber": 6
  },
  {
    "enable": true,
    "name": "Chapter/Section/Part 序号 标题",
    "rule": "^.{0,4}(?:[Cc]hapter|[Ss]ection|[Pp]art)\\s{0,4}\\d{1,4}.{0,30}$",
    "serialNumber": 7
  },
  {
    "enable": true,
    "name": "正文 标题/序号",
    "rule": "^\\s{0,4}正文\\s{1,4}.{0,20}$",
    "serialNumber": 8
  },
  {
    "enable": true,
    "name": "目录",
    "rule": "^[ \\t]{0,4}(?:简介|前言|序章|楔子|正文[^完结]|[Cc]hapter|[Ss]ection|终章|后记|尾声|番外|第?\\s{0,2}[\\d零一二两三四五六七八九十百千万壹贰叁肆伍陆柒捌玖拾佰仟]+?\\s{0,2}(?:章|节[^课]?|卷|集[^合和]?|部[^分]?|篇[^张]?)).{0,30}$",
    "serialNumber": 9
  },
  {
    "enable": true,
    "name": "目录(不匹配行前空白，正文不出现双标题)",
    "rule": "(?<=\\s)(?:简介|内容简介|前言|序章|楔子|正文[^完结]|[Cc]hapter|[Ss]ection|终章|后记|尾声|番外|第?\\s*[\\d零一二两三四五六七八九十百千万壹贰叁肆伍陆柒捌玖拾佰仟]+?\\s*(?:章|节[^课]?|卷|集[^合和]?|部[^分]?|篇[^张]?)).{0,30}$",
    "serialNumber": 10
  }
]